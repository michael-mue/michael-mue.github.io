---
import PublicationCardStyle from './PublicationCard.jsx';

const {
  authors,
  title,
  venue,
  year,
  pdf,
  cite,
  link,
  children
} = Astro.props;
---

<PublicationCardStyle
  authors={authors}
  title={title}
  venue={venue}
  year={year}
  pdf={pdf}
  cite={cite}
  link={link}
  client:load
>
  {children}
</PublicationCardStyle>
